<h1><%= @band.name %></h1>
<%#= render "shared/show", band: @band %>
<h2>Albums</h2>
<ul>
  <% @band.albums.each do |album| %>
    <li><%= album.name %></li>
  <% end %>
</ul>

<%= button_to "Edit Band", edit_band_url(@band), method: :get %>
<%= button_to "Delete Band", band_url(@band), method: :delete %>
<%= button_to "New #{@band.name} Album", new_band_album_url(@band), method: :get %>


